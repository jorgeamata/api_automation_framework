{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "rovers": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "name": {
              "type": "string"
            },
            "landing_date": {
              "type": "string"
            },
            "launch_date": {
              "type": "string"
            },
            "status": {
              "type": "string"
            },
            "max_sol": {
              "type": "integer"
            },
            "max_date": {
              "type": "string"
            },
            "total_photos": {
              "type": "integer"
            },
            "cameras": {
              "type": "array",
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer"
                    },
                    "name": {
                      "type": "string"
                    },
                    "rover_id": {
                      "type": "integer"
                    },
                    "full_name": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "id",
                    "name",
                    "rover_id",
                    "full_name"
                  ]
                }
              ]
            }
          },
          "required": [
            "id",
            "name",
            "landing_date",
            "launch_date",
            "status",
            "max_sol",
            "max_date",
            "total_photos",
            "cameras"
          ]
        }
      ]
